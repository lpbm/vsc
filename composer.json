{
	"name": "vsc/vsc",
	"type": "library",
	"description": "A small collection of PHP classes for RESTful development",
	"require": {
		"php": ">=5.3.0",
		"vsc/vsc": "dev-composer_migration"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/lpbm/vsc.git"
		}
	],
	"minimum-stability": "stable",
	"license": "MIT",
	"authors": [
		{
			"name": "Marius Orcsik",
			"email": "marius@habarnam.ro",
			"role": "developer"
		}
	],
	"autoload": {
		"psr-4" : { "vsc\\" : ["lib/", "res/"]}
	},
	"autoload-dev": {
		"psr-4" : { "_fixtures\\" : "tests/_fixtures"}
	}
}